                //Transaksi Penarikan Non Tunai
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Transaksi Penarikan Non Tunai"
                    android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                    android:textSize="15dp" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <LinearLayout
                        android:layout_marginEnd="10dp"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Perkiraan"
                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                            android:textSize="15dp" />
                        <com.google.android.material.textfield.TextInputLayout
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="10dp"
                            app:boxCornerRadiusBottomEnd="10dp"
                            app:boxCornerRadiusBottomStart="10dp"
                            app:boxCornerRadiusTopEnd="10dp"
                            app:boxCornerRadiusTopStart="10dp">

                            <AutoCompleteTextView
                                android:dropDownHeight="200dp"
                                android:focusable="false"
                                android:inputType="none"
                                android:id="@+id/act_perkiraan4"
                                android:layout_width="match_parent"
                                android:layout_height="35dp"
                                android:drawableTint="@color/black"
                                android:textSize="12dp"
                                android:paddingVertical="@dimen/dp_10"
                                android:includeFontPadding="false"
                                android:gravity="center_vertical"
                                android:singleLine="true"/>

                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginStart="10dp"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Frekuensi"
                            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
                            android:textSize="15dp" />
                        <com.google.android.material.textfield.TextInputLayout
                            style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="10dp"
                            app:boxCornerRadiusBottomEnd="10dp"
                            app:boxCornerRadiusBottomStart="10dp"
                            app:boxCornerRadiusTopEnd="10dp"
                            app:boxCornerRadiusTopStart="10dp">

                            <AutoCompleteTextView
                                android:dropDownHeight="200dp"
                                android:focusable="false"
                                android:inputType="none"
                                android:id="@+id/act_frekuensi4"
                                android:layout_width="match_parent"
                                android:layout_height="35dp"
                                android:drawableTint="@color/black"
                                android:textSize="12dp"
                                android:paddingVertical="@dimen/dp_10"
                                android:includeFontPadding="false"
                                android:gravity="center_vertical"
                                android:singleLine="true"/>

                        </com.google.android.material.textfield.TextInputLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:gravity="center_horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">
                    <Button
                        android:id="@+id/btnKembali_Keuangan"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="35dp"
                        android:layout_marginEnd="10dp"
                        android:background="@drawable/oval_background_10dp"
                        android:backgroundTint="@color/bg_cif_success"
                        android:text="@string/kembali"
                        android:textColor="@color/white"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                    <Button
                        android:id="@+id/btnProses_Keuangan"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="35dp"
                        android:layout_marginStart="10dp"
                        android:background="@drawable/oval_background_10dp"
                        android:backgroundTint="@color/Blue"
                        android:text="@string/btn_continue"
                        android:textColor="@color/white"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                </LinearLayout>
            </LinearLayout>
        </androidx.cardview.widget.CardView>
    </androidx.core.widget.NestedScrollView>
</RelativeLayout>